{
    "name": "rcarvs/ripcord",
    "description": "RPC client and server around PHP's xmlrpc library but without XMLRPC extension necessity",
    "keywords": ["RPC", "XML", "XMLRPC"],
    "require": {
        "php": ">=8.0",
        "phpxmlrpc/phpxmlrpc": "^4.9"
    },
    "autoload": {
        "psr-4": {
            "Ripcord\\": "src/Ripcord",
        },
        "files": ["src/Ripcord/functions.php"]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "rcarvs"
        }
    ],
    "minimum-stability": "dev",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14@dev"
    }
}
